    "autoprefixer": "^9",
    "gridsome": "^0.7.0",
    "postcss": "^8",
    "tailwindcss": "^3.2.7"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.9",
    "prettier": "2.8.4"
  }
}
